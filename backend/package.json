{
  "name": "rag-system-backend",
  "version": "1.0.0",
  "description": "RAG System Backend API with Gemini and Qdrant",
  "main": "dist/server.js",
  "scripts": {
    "dev": "vite-node src/server.ts --watch",
    "build": "tsc --noEmit && vite build",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "rag",
    "gemini",
    "qdrant",
    "typescript"
  ],
  "author": "Manav Vaishnani",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.39.0",
    "@prisma/client": "^5.20.0",
    "@qdrant/js-client-rest": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.3",
    "connect-timeout": "^1.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.8.0",
    "multer": "^1.4.5-lts.1",
    "opossum": "^9.0.0",
    "pdf-parse": "^1.1.1",
    "rate-limit-redis": "^4.3.1",
    "socket.io": "^4.8.1",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-timeout": "^1.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "@types/opossum": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "prisma": "^5.20.0",
    "rollup-plugin-node-externals": "^8.1.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-node": "^5.3.0"
  }
}
